@import nib

html, body {
    margin: 0;
    padding: 0;
    height: 100%;
    font-family: "Microsoft YaHei","微软雅黑";
    font-size: 100%;
}
@media screen and (max-width:880px){
    html, body {
        font-size: 75%;
    }
}
@media screen and (max-width:668px){
    html, body {
        font-size: 68.75%;
    }
}
@media screen and (max-width:380px){
    html, body {
        font-size: 62.5%;
    }
}

body {
    margin:0;
    padding:0;
    font-size: 1.4em;
    background: #eeeeee;
    color: #1a1a1a;
     -webkit-touch-callout: none !important;
}
* {
    box-sizing: border-box;
    -webkit-tap-highlight-color:transparent;
    outline:none;
}
i{
    font-style: normal;
}
a {
    user-select: none !important;
}
input {    
    outline: none;    
    -webkit-appearance: none;
    -moz-appearance: none;
}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button{
    -webkit-appearance: none !important;
    margin: 0; 
}
input,textarea{
    font-family: inherit;
    outline: none;
    color: inherit;
    font-style:normal;
    font-size:inherit;
}
ul.noStyle{
    list-style:none;
    padding:0;
    margin:0;
}
.btn{
    color:white;
    cursor:pointer;
    font-size:1.5rem;
    text-align:center;
    border: 0;
}
.btn.fe95{
    background-color:#fe9501;
}
.btn.fe51{
    background-color:#fe5100;
}
.btn.gray{
    background-color:#787878;
}
.mLayer{
    position:fixed;
    background-color:rgba(0,0,0,0.3);
    top:0;
    left:0;
    right:0;
    bottom:0;
    z-index:10001;
    text-align: center;
}
.mLayer>*{
    display:inline-block;
    vertical-align: middle;
}
.mLayer>.anchor{
    margin-right: 0;
    height:100%;
}
.mTip{
    position:fixed;
    top:0;
    left:0;
    right:0;
    bottom:0;
    z-index:10005;
    text-align:center;
}
.mTip>span{
    display:inline-block;
    vertical-align:middle;
}
.mTip>span:first-child{
    height:100%;
}
.mTip> span:last-child {
    background-color: rgba(0,0,0,0.8);
    color: white;
    padding: 1.5rem 1.5rem;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    width:70%;
    font-size:1.5rem;
}
.mLoading{
    background-color:rgba(0,0,0,0.3);
    position:fixed;
    top:0;
    left:0;
    right:0;
    bottom:0;
    z-index:10002;
    text-align: center;
    display:none;
}
.mLoading>.anchor{
    display: inline-block;
    height: 100%;
    vertical-align: middle;
}
#mAlert{
    background-color:rgba(0,0,0,0.3);
    position:fixed;
    top:0;
    left:0;
    right:0;
    bottom:0;
    z-index:20000;
    text-align: center;
    display:none;
}

#mAlert>span{
    display: inline-block;
    height: 100%;
    vertical-align: middle;
}

#mAlert > div {
    display: inline-block;
    background-color: rgba(255,255,255,0.9);
    width: 80%;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: 2px 2px 3px #aaaaaa;
    -webkit-box-shadow: 2px 2px 3px #aaaaaa;
    box-shadow: 2px 2px 3px #aaaaaa;
}

#mAlert .mMsg {
    padding: 2.5rem;
    border-bottom: 1px solid #b8b6b2;
    word-wrap: break-word;
    word-break: break-all;
}
.noScroll{
    overflow:hidden;
}
#mAlert .mBtn{
    color:#007aff;
    font-size:1.6rem;
    padding:1.5rem;
}

#mConfirm{
    background-color:rgba(0,0,0,0.3);
    position:fixed;
    top:0;
    left:0;
    right:0;
    bottom:0;
    z-index:10000;
    text-align: center;
    display:none;
}
#mConfirm>span{
    display: inline-block;
    height: 100%;
    vertical-align: middle;
}
#mConfirm > div {
    display: inline-block;
    background-color: rgba(255,255,255,0.9);
    width: 80%;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: 2px 2px 3px #aaaaaa;
    -webkit-box-shadow: 2px 2px 3px #aaaaaa;
    box-shadow: 2px 2px 3px #aaaaaa;
}
#mConfirm .mMsg {
    padding:2rem 2rem 1.5rem;
    border-bottom: 1px solid #b8b6b2;
    -ms-word-wrap: break-word;
    word-wrap: break-word;
    -ms-word-break: break-all;
    word-break: break-all;
}
#mConfirm .mBtn{
    font-size:1.6rem;
    color:#007aff;
}
#mConfirm .mBtn > span {
    padding:1.5rem;
    display: inline-block;
    width: 50%;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
#mConfirm .mBtn > span:first-child{
    border-right:1px solid #b8b6b2;
}

.arrowRight:after {
    content:"";
    display: inline-block;
    width: 1.5rem;
    height: 1.5rem;
    border-top: 2px solid #eeeeee;
    border-right: 2px solid #eeeeee;
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
@keyframes arrowRight {
	0%,30%{opacity: 0;transform: translateX(-8px);}
	60%{opacity: 1;transform: translateX(0);}
	100%{opacity: 0;transform: translateX(10px);}
}


