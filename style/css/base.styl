._floatTool{
    position: fixed;
    z-index: 555;
    bottom: 10rem;
    text-align: right;
    right: 1.5rem;
    width:6rem;
}
._floatTool.w{
    width:auto;
}
._floatTool.active > a.item {
    transform: translate3d(0,0,0) rotate(0deg) !important;
}
._floatTool > a {
    display: inline-block;
    vertical-align: middle;
    width: 4.5rem;
    height: 4.5rem;
    line-height: 4.5rem;
    text-align: center;
    -moz-border-radius: 4.5rem;
    -webkit-border-radius: 4.5rem;
    border-radius: 4.5rem;
    background-color: #fe5100;
    color: white;
    margin-left: 1.5rem;
    -moz-transition: all 0.3s cubic-bezier(.5,.15,.49,1.36);
    -o-transition: all 0.3s cubic-bezier(.5,.15,.49,1.36);
    -webkit-transition: all 0.3s cubic-bezier(.5,.15,.49,1.36);
    transition: all 0.3s cubic-bezier(.5,.15,.49,1.36);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 2.4rem;
}
._floatTool > a.anchor{
    width: 5rem;
    height: 5rem;
    line-height:5rem;
    position: relative;
    z-index: 1111;
    background-image:url(../img/g84.png);
    background-size: 2.6rem;
}
._floatTool > a.item:nth-child(1) {
    -moz-transform: translate3d(15.3rem,0,0) rotate(720deg); 
    -ms-transform: translate3d(15.3rem,0,0) rotate(720deg);
    -o-transform: translate3d(15.3rem,0,0) rotate(720deg); 
    -webkit-transform: translate3d(15.3rem,0,0) rotate(720deg); 
    transform: translate3d(18.3rem,0,0) rotate(720deg);
}
._floatTool > a.item:nth-child(2) {
    -moz-transform: translate3d(12.3rem,0,0) rotate(720deg);
    -ms-transform: translate3d(12.3rem,0,0) rotate(720deg);
    -o-transform: translate3d(12.3rem,0,0) rotate(720deg);
    -webkit-transform: translate3d(12.3rem,0,0) rotate(720deg);
    transform: translate3d(12.3rem,0,0) rotate(720deg);
}
._floatTool > a.item:nth-child(3) {
    -moz-transform: translate3d(5.3rem,0,0) rotate(720deg);
    -ms-transform: translate3d(5.3rem,0,0) rotate(720deg);
    -o-transform: translate3d(5.3rem,0,0) rotate(720deg); 
    -webkit-transform: translate3d(5.3rem,0,0) rotate(720deg);
    transform: translate3d(6.3rem,0,0) rotate(720deg);
}
._floatTool > a.home{
    background-image:url(../img/homeIcon.png);
}
._floatTool > a.my{
    background-size: 100% 100%;
    background-image:url(../img/myIcon.png?v=1);
}
._floatTool > a.car{
    background-image:url(../img/buyCarIcon.png);
}

._floatTool > a.car > i {
    display: inline-block;
    width: 1.8rem;
    height: 1.8rem;
    -moz-border-radius: 1.8rem;
    -webkit-border-radius: 1.8rem;
    border-radius: 1.8rem;
    background-color: white;
    color: #fe5100;
    line-height: 1.8rem;
    position: absolute;
    top: 0.5rem;
    right: 0.5rem;
    font-size: 1.2rem;
}


.spinner {
  height: 6rem;
  text-align: center;
  display:inline-block;
}
 
.spinner > div {
  background-color:white;
  height: 100%;
  width: 0.6rem;
  margin-right: 0.4rem;
  display: inline-block;
   
  -webkit-animation: stretchdelay 1.2s infinite ease-in-out;
  animation: stretchdelay 1.2s infinite ease-in-out;
}
 
.spinner .rect2 {
  -webkit-animation-delay: -1.1s;
  animation-delay: -1.1s;
}
 
.spinner .rect3 {
  -webkit-animation-delay: -1.0s;
  animation-delay: -1.0s;
}
 
.spinner .rect4 {
  -webkit-animation-delay: -0.9s;
  animation-delay: -0.9s;
}
 
.spinner .rect5 {
  -webkit-animation-delay: -0.8s;
  animation-delay: -0.8s;
}
 
@-webkit-keyframes stretchdelay {
  0%, 40%, 100% { -webkit-transform: scaleY(0.4) } 
  20% { -webkit-transform: scaleY(1.0) }
}
 
@keyframes stretchdelay {
  0%, 40%, 100% {
    transform: scaleY(0.4);
    -webkit-transform: scaleY(0.4);
  }  20% {
    transform: scaleY(1.0);
    -webkit-transform: scaleY(1.0);
  }
}